{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
    ],
    "rewrites": [
    {
      "source": "/src/img/(.*)",
      "destination": "/img/$1"
    }
  ]
  }