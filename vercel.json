{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/src/img/$1"
    }
      ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
