{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(:match*.png)",
            "dest": "/src/img/(:match*.png)"
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        }
    ]
}
