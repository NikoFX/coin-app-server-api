{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/src/img/$1",
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        }
    ]
}
