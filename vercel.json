{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/img",
            "dest": "https://google.com"
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        }
    ]
}
